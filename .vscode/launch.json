{
    // IntelliSense を使用して利用可能な属性を学べます。
    // 既存の属性の説明をホバーして表示します。
    // 詳細情報は次を確認してください: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Flutter Debug",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": ["--debug"]
        },
        {
            "name": "Flutter Release",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": ["--release"],
            "flutterMode": "release",
            "env": {
                "KEYSTORE_PASSWORD": "2198655",
                "KEY_PASSWORD": "2198655"
            }
        },
        {
            "name": "Flutter Profile",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": ["--profile"],
            "flutterMode": "profile"
        },
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        }
    ]
}